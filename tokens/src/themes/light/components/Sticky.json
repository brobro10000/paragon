{
  "elevation": {
    "$type": "shadow",
    "sticky": {
      "shadow": {
        "top": {
          "source": "$sticky-shadow-top",
          "$value": "0 -.5rem 1rem rgba(0, 0, 0, .15), 0 -.25rem .625rem rgba(0, 0, 0, .15)"
        },
        "bottom": {
          "source": "$sticky-shadow-bottom",
          "$value": "0 .5rem 1rem rgba(0, 0, 0, .15), 0 .25rem .625rem rgba(0, 0, 0, .15)"
        }
      }
    }
  }
}
